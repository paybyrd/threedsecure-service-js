    "@babel/plugin-transform-spread": "^7.19.0",
    "@babel/preset-env": "^7.19.1",
    "babel-preset-minify": "^0.5.2",
    "live-server": "1.2.2",
    "uglify-js": "^3.16.2"
  },
  "babel": {
    "presets": [
